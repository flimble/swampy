@using SAIG.PS.Swampy.Admin.MVC.Models
@model SAIG.PS.Swampy.Admin.MVC.Models.EnvironmentViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.environmentName</h2>
<table>
@foreach(EnvironmentViewModel.EndpointViewModel endpoint in Model.Endpoints)
{
    <tr>
        <td>@endpoint.Key</td>
        <td>@endpoint.Value</td>
        <td>@endpoint.Description</td>
        <td>@endpoint.Type</td>
    </tr>
}
</table>
